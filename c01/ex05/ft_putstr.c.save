#include <stdio.h>

int	str_count(char *str)
{
	char	ch;
	int		i;
	int	count;

	count = 0;
	ch = str[0];
	i = 0;
	while (ch != '\0')
	{
		count++;
		ch = str[i];
		i++;
	}
	return (0);
}

void	ft_putstr(char *str)
{
	write(1, str, str_count(str));
}

